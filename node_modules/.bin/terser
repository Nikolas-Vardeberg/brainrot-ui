#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Elev/Documents/GitHub/brainrot-ui/node_modules/terser/bin/node_modules:/mnt/c/Users/Elev/Documents/GitHub/brainrot-ui/node_modules/terser/node_modules:/mnt/c/Users/Elev/Documents/GitHub/brainrot-ui/node_modules:/mnt/c/Users/Elev/Documents/GitHub/node_modules:/mnt/c/Users/Elev/Documents/node_modules:/mnt/c/Users/Elev/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Elev/Documents/GitHub/brainrot-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Elev/Documents/GitHub/brainrot-ui/node_modules/terser/bin/node_modules:/mnt/c/Users/Elev/Documents/GitHub/brainrot-ui/node_modules/terser/node_modules:/mnt/c/Users/Elev/Documents/GitHub/brainrot-ui/node_modules:/mnt/c/Users/Elev/Documents/GitHub/node_modules:/mnt/c/Users/Elev/Documents/node_modules:/mnt/c/Users/Elev/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Elev/Documents/GitHub/brainrot-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
